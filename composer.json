{
    "name": "fadllabanie/env-editor",
    "description": "A Laravel package to edit .env files from a web interface.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fadl Labanie",
            "email": "labanie.fadl@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Fadllabanie\\EnvEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fadllabanie\\EnvEditor\\EnvEditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fadllabanie/env-editor"
        }
    ]
}
