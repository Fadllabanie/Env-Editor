{
    "name": "fadllabanie/env-editor",
    "description": "A Laravel package to edit .env files from a web interface.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fadl Labanie",
            "email": "labanie.fadl@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^11.0"  
    },
    "autoload": {
        "psr-4": {
            "Fadllabanie\\EnvEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fadllabanie\\EnvEditor\\EnvEditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
